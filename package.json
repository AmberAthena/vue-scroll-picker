{
  "name": "vue-scroll-picker",
  "version": "0.1.4",
  "description": "scroll picker component for Vue.js.",
  "author": "Changwan Jun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/vue-scroll-picker.git"
  },
  "scripts": {
    "dev": "vue build build/dev.js --config build/vue.config.js --host 0.0.0.0",
    "build": "npm run build:production && npm run build:samples",
    "build:production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.config.js",
    "build:samples": "npm run build:samples:default",
    "build:samples:default": "webpack --config ./samples/default/webpack.config.js"
  },
  "main": "dist/index.js",
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
